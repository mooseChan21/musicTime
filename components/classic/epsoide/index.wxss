/* components/classic/epsoide/index.wxss */
.container{
    display: inline-flex;
    flex-direction: row;
    height:60rpx;
}
.index-container{
    display: flex;
    flex-direction: row;
/* 底部对齐 */
    align-items: baseline;
}
.plan-txt{
    font-size: 32rpx
}
.index{
    font-size: 60rpx;
    line-height: 60rpx;
    margin-right: 16rpx;
    font-weight: 800
}
.line{
    height: 44rpx;
    margin-right: 14rpx;
    border-left: 1px solid #000;
}
.date-container{
    display: flex;
    flex-direction: column;
    margin-top: 5rpx
}
.month{
    font-size: 24rpx;
    line-height: 24rpx
}
.year{
    font-size: 20rpx;
}